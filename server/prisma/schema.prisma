generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  username  String
  email     String   @unique
  password  String
  githubId  String?  @unique
  discordId String?  @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
